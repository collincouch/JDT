@{
    ViewBag.Title = "Just Do It";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="page-title" id="workOutTitle"></h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-10">
            <section class="widget">
                <header>
                    <h5>
                        <i class="fa fa-check-square-o"></i>
                        Just Do it
                    </h5>
                </header>
                <div class="body">
                    <div class="panel-group" id="accordion3">

                    </div>
                </div>
            </section>
        </div>
    </div>
</div>

@section styles{
    @Styles.Render("~/Content/css/stopwatch/bundles")
    }


@section scripts{
    @Scripts.Render("~/bundles/dashboard/jqueryplugins")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/site")
    @Scripts.Render("~/Content/js/CrudOps/Acct/Locker/JustDoItWK.js")
    @Scripts.Render("~/bundles/jqueryplugins")

    <script>
        $(document).ready(function () {
            initListAuth();

        });

    </script>
}
