
@{
    ViewBag.Title = "Workouts";
}
<div class="content container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="page-title">Your Workouts<small>View, Create, Edit from here.</small></h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-10">
            <section class="widget">
                <header>
                    <h4>
                        <i class="fa fa-list-alt"></i>
                        Workouts
                    </h4>
                </header>
                <div class="body">
                    <table class="table table-striped table-bordered" id="datatable-table">
                        <thead>
                            <tr>
                                <th>
                                    Name
                                </th>
                                <th class="hidden-xs">
                                    Description
                                </th>
                                <th class="hidden-xs">
                                    Date Created
                                </th>
                                <th>
                                    # Exercises
                                </th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                   
                </div>
            </section>
        </div>
    </div>
</div>
@section scripts{
    @Scripts.Render("~/bundles/dashboard/jqueryplugins")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/site")
@Scripts.Render("~/Content/js/CrudOps/WorkOuts.js")
@Scripts.Render("~/bundles/jqueryplugins")
   
<script>
    $(document).ready(function () {
        initListAuth();
    });

</script>



}

