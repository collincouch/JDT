
@{
    ViewBag.Title = "Plans";
}
<div class="content container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="page-title">Your Plans<small>View, Create, Edit from here.</small></h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-10">
            <section class="widget">
                <header>
                    <h4>
                        <i class="fa fa-list-alt"></i>
                        Plans
                    </h4>
                </header>
                <div class="body">

                    <table class="table table-striped table-bordered" id="datatable-table" width="100%">
                        <thead>
                            <tr>
                                <th>
                                    Name
                                </th>
                                <th>
                                    Description
                                </th>
                                <th>
                                    Date Created
                                </th>
                                <th>
                                    # Workouts
                                </th>
                            </tr>
                        </thead>
                       
                    </table>
                </div>
            </section>
        </div>
    </div>
</div>

@section scripts{
@Scripts.Render("~/bundles/dashboard/jqueryplugins")
@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/bundles/site")
@Scripts.Render("~/Content/js/CrudOps/Plans.js")
@Scripts.Render("~/bundles/jqueryplugins")

<script>
    $(document).ready(function () {
        initListAuth();
        
    });

</script>
}
